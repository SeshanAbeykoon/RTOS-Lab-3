//Blink LED in ESP32
void setup()
{
    // Set LED as output
    pinMode(LED_BUILTIN, OUTPUT);
    // Serial monitor setup
    Serial.begin(115200);
}

void loop()
{
    Serial.print("Blink LED");
    digitalWrite(LED_BUILTIN, HIGH);
    delay(500);
    digitalWrite(LED_BUILTIN, LOW);
    delay(500);
}
